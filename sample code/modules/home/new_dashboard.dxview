<div data-options="dxView : { name: 'new_dashboard', title: 'MAIN DASHBOARD', disableCache: true } ">
    <div data-options="dxContent : { targetPlaceholder: 'content' } " class="dx-content-background">

        <div id="toolbar" data-bind="dxToolbar: { dataSource: toolbarItems, visible: vs_dxtoolbar }"></div>


        <!--<div data-bind="dxScrollView: { height: get_dxtile_height }">
            <div align="center"><strong>GLICO Healthcare</strong></div>
